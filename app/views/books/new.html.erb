<%= form_with model: Book.new, local: true do |f| %>

  <h4>Name</h4>
  <%= f.text_field :title %>

  <h4>Image</h4>
  <%= f.file_field :image, accept: "image/*" %>

  <h4>Introduction</h4>
  <%= f.text_area :body %>

  <div class="form-group">
    <%= f.submit "Create Book", class: "btn btn-secondary" %>
  </div>

<% end %>


